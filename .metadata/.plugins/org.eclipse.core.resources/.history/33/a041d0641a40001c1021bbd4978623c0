package plsql;

import java.sql.CallableStatement;
import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.SQLException;
import java.sql.Types;

public class CallInoutProc {

	public static void main(String[] args) throws ClassNotFoundException, SQLException {
		// DB 접속
//	    1. Driver 연결
			Class.forName("oracle.jdbc.driver.OracleDriver");//다리
			String url = "jdbc:oracle:thin:@localhost:1521:orcl";//주소
		
		// 변수 선언
	      String p1value = new String("a");//in
	      String p2value = new String("b");//inOut
	      String p3value;//Out
	      
//		   2. 계정 연결
			Connection conn = DriverManager.getConnection(url, "javadb", "javadb");
			CallableStatement cs = conn.prepareCall("{call javatest(?,?,?)");
	        cs.setString(1, p1value);
	        cs.setString(2, p2value);
	        cs.registerOutParameter(2, Types.STRUCT);
	        cs.registerOutParameter(3, Types.STRUCT);
	        
	        cs.execute();
	        
	        p2value=cs.getString(1);
	        p3value=cs.getString(2);
	      
	   // 수행결과 출력
	      System.out.println("p2 :" + p2value);
	      System.out.println("p3 :" + p3value);

	}

}
